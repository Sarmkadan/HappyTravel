{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "HappyTravel.StdOutLogger.HttpContextLoggingMiddleware": "Information"
    },
    "Sentry": {
      "Endpoint": "HTDC_EDO_SENTRY_ENDPOINT"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Edo": "Server={0};Port={1};Database=edo;Userid={2};Password={3};"
  },
  "Edo": {
    "Database": {
      "Options": "edo/connection-string"
    },
    "Google": {
      "Endpoint": "https://maps.googleapis.com/maps/api/",
      "Options": "edo/google"
    },
    "Email": {
      "Options": "edo/email",
      "ApiKey": "apiKey",
      "SenderAddress": "senderAddress",
      "CustomerInvitationTemplateId": "invitationTemplateId",
      "AdministratorInvitationTemplateId": "adminInvitationTemplateId",
      "ExternalPaymentsTemplateId": "externalPaymentsTemplateId",
      "MasterCustomerRegistrationTemplateId": "masterCustomerRegistrationTemplateId",
      "RegularCustomerRegistrationTemplateId": "regularCustomerRegistrationTemplateId",
      "UnknownCustomerBillTemplateId": "unknownCustomerBillTemplateId",
      "KnownCustomerBillTemplateId": "knownCustomerBillTemplateId",
      "NeedPaymentTemplateId": "needPaymentTemplateId",
      "BookingCancelledTemplateId": "bookingCancelledTemplateId",
      "Administrators": "administrators",
      "BookingVoucherTemplateId": "bookingVoucherTemplateId",
      "BookingInvoiceTemplateId": "bookingInvoiceTemplateId",
      "EdoPublicUrl": "edoPublicUrl"
    },
    "Payfort": {
      "Options": "edo/payfort",
      "Urls": "edo/payfort/urls"
    }
  },
  "Authority": {
    "Options": "edo",
    "ApiName": "apiName",
    "Endpoint": "endpoint"
  },
  "DataProviders": {
    "Options": "edo/data-providers",
    "NetstormingConnector": "netstormingConnector"
  },
  "PaymentLinks": {
    "Options": "edo/payment-links",
    "ServiceTypes": {
      "HTL": "Hotel Booking",
      "TRN": "Transfer",
      "CMS": "Custom service"
    },
    "Currencies": [
      "AED",
      "SAR",
      "USD"
    ],
    "Endpoint": "endpoint"
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "edo",
    "Token": "HTDC_VAULT_TOKEN"
  }
}